@model GeoModalData
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<link href="~/lib/bootstrap-table/dist/bootstrap-table.css" rel="stylesheet" />

<div class="text-center">
    <h4>Welcome - Bootstrap table demo</h4>
</div>
<table id="myTable" data-toggle="table" data-pagination="true"
       data-search="true"
       data-show-toggle="true"
       data-show-columns="true">
    <thead>
        <tr class="tr-class-1">
            <th data-sortable="true" data-field="id">Name</th>
            <th>Id</th>
            <th>Category</th>
            <th data-sortable="true">Created On</th>
            <th>Geo Location</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var v in Model.Venues)
        {
            <tr>
                <td>@v.Name</td>
                <td>@v.Id</td>
                <td>@v.Category</td>
                <td>@v.CreatedDate</td>
                <td>@v.GeoLocationAddress</td>
            </tr>
        }
    </tbody>
</table>
<script src="~/lib/bootstrap-table/dist/bootstrap-table.js"></script>